{
  "extends": "airbnb",
  "globals": {
    "Emitter": true,
    "Settings": true,
    "WindowManager": true
  },
  "env": {
    "mocha": true
  },
  "parser": "babel-eslint",
  "rules": {
    "max-len": [1, 200],
    "import/no-unresolved": 0,
    "global-require": 0,
    "no-underscore-dangle": 0,
    "consistent-return": 0,
    "react/prefer-stateless-function": 0,
    "react/jsx-no-bind": 0
  }
}
